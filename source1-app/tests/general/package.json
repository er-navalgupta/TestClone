  "main": "index.js",
  "scripts": {
    "install:lib": "node installer.js",
    "install:eshtml": "npm i ../../eshtml-0.1.0.tgz",
    "install:build": "npm run install:lib && npm run build",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider; webpack --config webpack.config.js",
    "test:e2e": "mocha test/index.js --exit",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "cross-env NODE_ENV=test jest --runInBand",
    "build:test": "npm run build && npm run test",
    "build:test:dev": "NODE_ENV=development npm run build:test",
    "build:test:prod": "NODE_ENV=production npm run build:test",
    "install:build:test": "npm run install:lib &&  npm run build:test",
    "webpack:watch": "webpack --config webpack.config.js --watch",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider; npm run webpack:watch",
    "start:dev": "NODE_ENV=development npm run start",
    "start:prod": "NODE_ENV=production npm run start"
  },
